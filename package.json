{
  "name": "@follox42/jslogger",
  "version": "1.0.2",
  "description": "Python‑style logging for TypeScript – familiar API, typed ergonomics.",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p .",
    "prepack": "npm run build",
    "dev": "ts-node --esm src/index.ts",
    "test": "node --test",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "logging",
    "typescript"
  ],
  "repository": "git+https://github.com/follox42/jslogger.git",
  "author": "Follox follox.f@shosai.fr",
  "license": "MIT",
  "devDependencies": {
    "vitest": "^3.2.3"
  }
}
